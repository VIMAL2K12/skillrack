#include<stdio.h>
#include<stdlib.h>

void printPattern(int index,int i,char str[],int indicator)
{
    int t;
    if(indicator==1)
    {
        t=index;
        index=i;
        i=t;
    }
    for(;index>=i;index--) printf("%c",str[index]);
}

int main()
{
    int lenght,i,start,end;
    char str[101];
    scanf("%s%n",str,&lenght);
    start=0,end=lenght-1;
    for(i=0;i<lenght;i++,start++,end--,printf("\n"))
    {
        printPattern(start,0,str,0);
        printPattern(end,lenght-1,str,1);
    }
}

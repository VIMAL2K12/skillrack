n=int(input())
team,stadium=[],[]
for _ in range(n):
    s=input().split('-')
    team.append(s[0].strip())
    stadium.append(s[1].strip())
for i in range(n):
    for j in range(i+1,n):
        print(team[i]+" vs "+team[j]+' ('+stadium[i]+')')
        print(team[j]+" vs "+team[i]+' ('+stadium[j]+')')
